{
  "name": "fysh-sea",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "pkg/fysh-sea",
  "namedInputs": {
    "ignore": [
      "!{projectRoot}/**/*.md",
      "!{projectRoot}/LICENSE",
      "!{projectRoot}/dprint.json",
      "!{projectRoot}/examples"
    ]
  },
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "inputs": ["ignore"],
        "outputs": [
          "{projectRoot}/build/fysh-sea"
        ],
        "cwd": "pkg/fysh-sea",
        "command": "make fysh-sea",
        "color": true
      },
      "cache": true
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "inputs": ["ignore"],
        "cwd": "pkg/fysh-sea",
        "command": "make test",
        "color": true
      }
    }
  }
}

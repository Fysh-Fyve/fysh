{
  "name": "fysh-sea",
  "sourceRoot": "pkg/fysh-sea",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "inputs": [
          "!{projectRoot}/**/*.md",
          "!{projectRoot}/LICENSE",
          "!{projectRoot}/dprint.json",
          "!{projectRoot}/examples"
        ],
        "outputs": [
          "{projectRoot}/build/fysh-sea"
        ],
        "cwd": "pkg/fysh-sea",
        "command": "make fysh-sea",
        "color": true
      },
      "cache": true
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "inputs": [
          "!{projectRoot}/**/*.md",
          "!{projectRoot}/LICENSE",
          "!{projectRoot}/dprint.json",
          "!{projectRoot}/examples"
        ],
        "cwd": "pkg/fysh-sea",
        "command": "make test",
        "color": true
      },
      "cache": true
    }
  }
}

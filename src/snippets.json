{
  "Submarine": {
    "prefix": ["sub", "func"],
    "body": [">(${1:name}) ${2:params}", "><>", "\t$0", "<><"],
    "description": "A Submarine."
  },
  "Greater than tadpole": {
    "prefix": [">"],
    "body": ["o~"],
    "description": "Greater than tadpole."
  },
  "Greater than or equal tadpole": {
    "prefix": [">="],
    "body": ["o≈"],
    "description": "Greater than or equal tadpole."
  },
  "Less than tadpole": {
    "prefix": ["<"],
    "body": ["~o"],
    "description": "Less than tadpole."
  },
  "Less than or equal tadpole": {
    "prefix": ["<="],
    "body": ["≈o"],
    "description": "Less than or equal tadpole."
  },
  "Assign": { "prefix": ["="], "body": ["≈"], "description": "Assign." },
  "Loopy Fysh": {
    "prefix": ["loop", "@"],
    "body": ["><(((@> [${1:condition}]", "><>", "\t$0", "<><"],
    "description": "A Loopy Fysh."
  },
  "Happy Fysh": {
    "prefix": ["if"],
    "body": ["><(((^> [${1:condition}]", "><>", "\t$0", "<><"],
    "description": "A Happy Fysh."
  },
  "Dead Fysh": {
    "prefix": ["else"],
    "body": ["><(((*>", "><>", "\t$0", "<><"],
    "description": "A Happy Fysh."
  },
  "Elif Fysh": {
    "prefix": ["elif"],
    "body": ["><(((*> ><(((^> [${1:condition}]", "><>", "\t$0", "<><"],
    "description": "A Happy Fysh."
  },
  "Call Statement": {
    "prefix": ["cc"],
    "body": ["[$1] ~$0"],
    "description": "A single call statement."
  },
  "Identifier": {
    "prefix": ["id"],
    "body": ["><${1:name}> $0"],
    "description": "A Fysh Identifier."
  }
}

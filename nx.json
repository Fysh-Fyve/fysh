{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "workspaceLayout": {
    "appsDir": "pkg",
    "libsDir": "pkg"
  },
  "targetDefaults": {
    "test": {
      "cache": true
    },
    "lint": {
      "cache": true
    }
  },
  "plugins": [
    "@nx-go/nx-go"
  ],
  "namedInputs": {
    "sharedGlobals": [
      "{workspaceRoot}/go.work"
    ]
  }
}

{
  "$schema": "vscode://schemas/color-theme",
  "name": "Fysh",
  "include": "./defaults/dark_plus.json",
  "tokenColors": [
    {
      "scope": "keyword.control.fysh",
      "settings": { "foreground": "#00ffe5" }
    },
    {
      "scope": "keyword.control.loop.break.fysh",
      "settings": { "foreground": "#d14844" }
    },
    {
      "scope": "comment.block.fysh",
      "settings": { "foreground": "#4FFF64", "fontStyle": "italic" }
    },
    {
      "scope": "comment.line.fysh",
      "settings": { "foreground": "#4FFF64", "fontStyle": "italic" }
    },
    {
      "scope": "entity.name.function.fysh",
      "settings": { "foreground": "#FEFE3B" }
    },
    {
      "scope": "variable.fysh.right",
      "settings": { "foreground": "#3BB2F5" }
    },
    {
      "scope": "variable.fysh.right.increment",
      "settings": { "foreground": "#4E82BE" }
    },
    {
      "scope": "variable.fysh.left",
      "settings": { "foreground": "#ff7d7d" }
    },
    {
      "scope": "variable.fysh.left.decrement",
      "settings": { "foreground": "#C24F4C" }
    },
    {
      "scope": "constant.numeric.fysh.right",
      "settings": { "foreground": "#3BB2F5" }
    },
    {
      "scope": "constant.numeric.fysh.left",
      "settings": { "foreground": "#ff7d7d" }
    },
    {
      "scope": "constant.numeric.fysh.right.scale",
      "settings": { "foreground": "#5380B7" }
    },
    {
      "scope": "constant.numeric.fysh.left.scale",
      "settings": { "foreground": "#f44c4c" }
    },
    {
      "scope": "constant.numeric.fysh.left.scale.zero",
      "settings": { "foreground": "#F5CBCC" }
    },
    {
      "scope": "constant.numeric.fysh.right.scale.zero",
      "settings": { "foreground": "#DAE5F1" }
    },
    {
      "scope": "keyword.operator.arithmetic.multiply.fysh",
      "settings": { "foreground": "#F79FF3", "fontStyle": "bold" }
    },
    {
      "scope": "source.fysh keyword.operator",
      "settings": { "foreground": "#d18ece" }
    },
    {
      "scope": "support.function.grilled.fysh",
      "settings": { "foreground": "#94691C" }
    },
    {
      "scope": "support.function.grilled.fysh.body",
      "settings": { "foreground": "#9A9A9A" }
    },
    {
      "scope": "keyword.control.return.fysh",
      "settings": { "foreground": "#00ffe5" }
    },
    {
      "scope": "punctuation.terminator.fysh",
      "settings": { "foreground": "#bafffe" }
    }
  ]
}

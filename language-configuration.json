{
  "comments": {
    "lineComment": "><//>",
    "blockComment": ["></*>", "<*/><"]
  },
  "brackets": [
    ["><>", "<><"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "><>", "close": "<><" },
    { "open": "[", "close": "]" },
    { "open": "></*>", "close": "<*/><" }
  ],
  "autoCloseBefore": "\n\t",
  "surroundingPairs": [
    ["><>", "<><"],
    ["[", "]"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*><//>\\s*#?region\\b",
      "end": "^\\s*><//>\\s*#?endregion\\b"
    }
  }
}

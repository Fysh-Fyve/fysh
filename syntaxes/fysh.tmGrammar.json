{
  "scopeName": "source.fysh",
  "fileTypes": [ "fysh" ],
  "name": "Fysh",
  "patterns": [
  {"include": "#terminator"},
  {"include": "#comment.line.fysh"},
  {"include": "#block-comment"},
  {"include": "#left-identifier"},
  {"include": "#decrement"},
  {"include": "#right-identifier"},
  {"include": "#increment"},
  {"include": "#control-fysh"},
  {"include": "#assignment.operator.fysh"},
  {"include": "#multiply.operator.fysh"},
  {"include": "#anchor.fysh"},
  {
      "match": "~",
      "name": "terminator"
  },
  { 
    "match": "><//>.*\n",
    "name": "comment.line.fysh"
  },
  {
    "match": "[‚âà=]",
    "name": "assignment.operator.fysh"
  },
  {
    "match": "[‚ô°‚ô•‚ù§üéî‚ù•‚ù£‚ù¶‚ùß‚òô]",
    "name": "multiply.operator.fysh"
  },
  {
    "match": "(?:\\(\\+o|o\\+\\))",
    "name": "anchor.fysh"
  }
  ],
  "repository": {
    "left-identifier": {
      "match": "<[^[*^@]\\{\\}()->][^\\{\\}()>]*><(?!<)",
      "name": "variable.fysh.left"
    },
    "decrement": {
      "match": "<[^[*^@]\\{\\}()->][^\\{\\}()>]*><<",
      "name": "variable.fysh.decrement"
    },
    "right-identifier": {
      "match": "><[^[*^@]\\{\\}()->][^\\{\\}()>]*>",
      "name": "variable.fysh.right"
    },
    "increment": {
      "match": ">><[^[*^@]\\{\\}()->][^\\{\\}()>]*>",
      "name": "variable.fysh.increment"
    },
    "block-comment": {
      "begin": "></\\*>",
      "end": "<\\*/><",
      "name": "comment.block.fysh"
    },
    "control-fysh": {
      "name": "keyword.control.fysh",
      "match": "><\\(*[*^@]>"
    }
  }
}

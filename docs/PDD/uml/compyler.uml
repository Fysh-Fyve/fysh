@startuml
node "Dev Machine" {
  frame "Front end" as FE {
    [FyshLexer]
    [FyshParser]
    [Semantic Analyzer] as SA
  }

  frame "LLVM Compiler Infrastructure" as ME {
    [Intermediate Representation] as IR
    [Optimization Passes] as OP
    [Code Generator] as CG
  }
}

FyshLexer -right-> FyshParser
FyshParser -right-> SA
SA -down-> (IR)
IR -right-> (OP)
OP -right-> (CG)
@enduml
